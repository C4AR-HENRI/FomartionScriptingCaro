function get_rotation(_peg){
    var rotation = Node.getTextAttr(_peg,frameElement.current(),"ROTATION.ANGLEZ")
    return rotation
}

function set_rotation(_peg,angle){
    var rotation = Node.setTextAttr(_peg,"ROTATION.ANGLEZ",frameElement.current(),angle)
    MessageBox.information()

}

function bt_rotate_90_deegrees(){
    var _peg = selection.selectedNode(0) 
    node.setTextAttr(_peg,"ROTATION.ANGLEZ",frameElement.current(),"90")
}